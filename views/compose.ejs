<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label class="form-label" for="postTitle">Title</label>
        <input class="form-control" type="text" name="postTitle" id="postTitle">
        <label class="form-label" for="postBody">Post</label>
        <textarea class="form-control" name="postBody" id="postBody"  rows="6"></textarea>
    </div>
    <br>
    <button class="btn btn-dark" type="submit" name="submit">Publish</button>
</form>

<%- include("partials/footer") -%>